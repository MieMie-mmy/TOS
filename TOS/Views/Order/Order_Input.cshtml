                                    T_OrderDetail.AdminCD = adminCD;
                                    T_OrderDetail.OrderItem = ordercount;
                                    T_OrderDetail.StockItem = inventorycount;
                                    T_OrderDetail.SalePrice = price;
                                    T_OrderDetail.Memo = ItemMemo;
                                    T_OrderDetail.TotalAmount = ordercount * price;
 
                                    if (skumemo == null || skumemo == "") {
                                        T_OrderDetail.Memo = ItemMemo;
                                    }
                                    else {
                                        T_OrderDetail.Memo = skumemo;
                                    }
                                    T_OrderDetail.AvailableShippingDate = $("#shipDate").val().toString();
                                    order.push(T_OrderDetail);
                                }
                            }

                    }
                    i = i + 1;
                  
                }

            }
            $.ajax({
                url: "../../Order/InserOrder",
                type: 'post',
                dataType: 'json',
                contentType: 'application/json',
                data: JSON.stringify({ T_OrderHeader: T_OrderHeader, T_OrderDetail: order }),
                cache: false,
                success: function (result) {
                    alert(result);
                },
                error: function (xhr, ajaxOptions, thrownError) {
                    alert(thrownError);
                }
            });
           

