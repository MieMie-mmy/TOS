<script>
    function odFunction() {
        var x = document.getElementById("mailbox-content");
        if (x.style.display === "block") {
            x.style.display = "none";

        } else {
            x.style.display = "block";
        }
    }
    $("#btnb").click(function () {
        $(this).text() === 'ブロック最小化'
            ? $(this).text('ブロック最小化')
            : $(this).text('ブロック表示')
    })
</script>



