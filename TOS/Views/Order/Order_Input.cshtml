                T_OrderHeader.Address1 = $("#add1").val();
                T_OrderHeader.Address2 = $("#add2").val();
                T_OrderHeader.TotalAmount = totalamount;
                T_OrderHeader.Memo = $("#txtsummary").val();

            }
            else
            {
                T_OrderHeader.OrderID = orderid;
                T_OrderHeader.ShippingID = shipid;
                T_OrderHeader.ZipCD1 = "";
                T_OrderHeader.ZipCD2 = "";
                T_OrderHeader.TelephoneNo = "";
                T_OrderHeader.ShippingName = "";
                T_OrderHeader.Address1 = "";
                T_OrderHeader.Address2 = "";
                T_OrderHeader.TotalAmount = totalamount;
                T_OrderHeader.Memo = $("#txtsummary").val();
            }        
            var order = new Array();
            for (var k = 1; k <= c; k++)
            {       
              
                var price = $('#makeritem' + k).find("tr:eq(1)").find("td:eq(5)").html();
                var ItemMemo = $('#makeritem' + k).find("tr:eq(1)").find("td:eq(7)").find('input[type=text]').html();
                alert(price);
                var table = document.getElementById("sku"+k).getElementsByClassName("tt")[0];
                var text = "";
                for (var i = 1, row; row = table.rows[i]; i++) {
                  
                    for (var j = 0, col; col = row.cells[j]; j++) {
                       
                            if (j == 0) {
                                var skumemo = table.rows[i].cells[0].getElementsByClassName("form-control")[0].value;
                                j = j + 1;
                            }
                            else
                            {
                                var ordercount = table.rows[i + 1].cells[j].getElementsByClassName("form-control sair")[0].value;
                               
                                if (ordercount != null && ordercount > 0)
                                {
                                    alert(ordercount);
                                    var inventorycount = table.rows[i].cells[j].getElementsByClassName("InventoryCount")[0].textContent;
                                    var adminCD = table.rows[i].cells[j].getElementsByClassName("admincd")[0].textContent;
                                    var T_OrderDetail = {}
