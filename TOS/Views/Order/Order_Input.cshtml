        $.makeTable = function (mydata) {
            var table = $('<table class="tt">');
            var tblHeader = "<tr class ='odrinfo'>";
            tblHeader += "<td class='t300 saict tab002tit'>摘要</td>";
            for (var k in mydata[0]) tblHeader += "<td class='t300 tab002tit saict'>" + k + "</td>";
            tblHeader += "</tr>";
            $(tblHeader).appendTo(table);
            $.each(mydata, function (index, value) {

                var TableRow = "<tr class='saisinHeading odrinfo'>";
                TableRow += "<td width='20%'><input class='form-control' type='text' placeholder='Aさんの分' /></td>"

                $.each(value, function (key, val) {
                    if (val.indexOf(',') > -1) {
                        var tmp = val.split(",");
                        //TableRow += "<td class='saict'>" + tmp[0] + "<label class='admincd' style='visibility: hidden'>" + tmp[1] + "</label></td>";
                        TableRow += "<td class='saict'> <label class='InventoryCount'> " + tmp[0] + "</label> <label class='admincd' style='visibility: hidden'>" + tmp[1] + "</label></td>";
                    }
                    else
                    {
                        TableRow += "<td class='saict'>" + val + "</td>";

                    }
                    //var tmp = val.split(",");
                    //TableRow += "<td class='saict'>" + tmp[0] + "<label class='admincd'>"+ tmp[1]+ "</label></td>";
                });
                TableRow += "</tr>";
                $(table).append(TableRow);

                //var Tablefixfrow = "<tr>< td width='30px' class='t30 saict tab002tit'> 摘要</td ><td width='30px' class='t30 tab002tit saict'>発注数</td><td class='odr6'><input class='t30 form-control' type='text' value='0' /></td><td class='odr6'><input class='t30 form-control' type='text' value='0' /></td></tr>";
                var Tablefixfrow = "<tr><td class='t30 saict tab002tit'> 摘要</td >";
                Tablefixfrow += "<td class='t30 tab002tit saict'>発注数</td>";
                var count = Object.keys(mydata[0]).length;
                //alert(count);
                for (var i = 1; i < count; i++) {
                    Tablefixfrow += " <td class='odr6'><input class='form-control sair' type='text' placeholder='0'/></td>";
                };
                Tablefixfrow += "</tr > ";
                $(table).append(Tablefixfrow);
            });
            return ($(table));
        };

